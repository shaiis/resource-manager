{
  "name": "resource-manager",
  "version": "1.0.0",
  "main": "lib/resource.manager.js",
  "scripts": {
    "test": "mocha tests/** -r blanket -R travis-cov",
    "testcovhtml": "mocha tests/** -r blanket -R html-cov > coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaiis/resource-manager"
  },
  "dependencies": {
    "underscore": "1.5.2",
    "q": "0.9.7"
  },
  "devDependencies": {
    "mocha": "1.18.0",
    "chai": "1.8.0",
    "chai-as-promised": "4.1.1",
    "blanket": "^1.1.6",
    "travis-cov": "^0.2.5"
  },
  "config": {
    "blanket": {
      "pattern": "/lib/",
      "data-cover-never": "node_modules"
    },
    "travis-cov": { "threshold": 99 }
  }
}
